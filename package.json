{
  "name": "cypress-automation",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "cypress run",
    "install:deps": "npm install",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:chrome": "cypress run --browser chrome",
    "cy:run:firefox": "cypress run --browser firefox",
    "cy:run:headed": "cypress run --headed",
    "test:api:dev": "cypress run --env environment=dev",
    "test:api:stage": "cypress run --env environment=stage",
    "test:api:prod": "cypress run --env environment=prod",
    "test:dev-only": "cypress run --env grepTags='@dev'",
    "test:coverage": "cypress run --env coverage=true",
    "test:parallel:context": "node scripts/parallel-test-runner.js --strategy=context",
    "test:parallel:tags": "node scripts/parallel-test-runner.js --strategy=tags",
    "test:parallel:specs": "node scripts/parallel-test-runner.js --strategy=specs",
    "test:api": "cypress run --spec 'cypress/e2e/api/**/*.cy.js'",
    "test:integration": "cypress run --spec 'cypress/e2e/integration/**/*.cy.js'",
    "test:comprehensive-all": "cypress run --spec 'cypress/e2e/api/**/*.cy.js' --env grepTags='@comprehensive'",
    "test:error-handling": "cypress run --spec 'cypress/e2e/api/error-handling-comprehensive.cy.js'",
    "test:security": "cypress run --env grepTags='@security'",
    "test:performance": "cypress run --env grepTags='@performance'",
    "test:validation": "cypress run --env grepTags='@validation'",
    "test:parallel": "npm-run-all --parallel test:account test:auth test:iam",
    "test:account": "cypress run --spec 'cypress/e2e/api/account-*.cy.js'",
    "test:auth": "cypress run --spec 'cypress/e2e/api/auth-*.cy.js'",
    "test:iam": "cypress run --spec 'cypress/e2e/api/iam-*.cy.js'",
    "test:edge-application": "cypress run --spec 'cypress/e2e/api/edge-application-*.cy.js'",
    "test:edge-firewall": "cypress run --spec 'cypress/e2e/api/edge-firewall-*.cy.js'",
    "test:orchestrator": "cypress run --spec 'cypress/e2e/api/orchestrator-*.cy.js'",
    "test:purge": "cypress run --spec 'cypress/e2e/api/real-time-purge.cy.js'",
    "test:dns": "cypress run --spec 'cypress/e2e/api/dns-*.cy.js'",
    "test:waf": "cypress run --spec 'cypress/e2e/api/waf-*.cy.js'",
    "test:storage": "cypress run --spec 'cypress/e2e/api/edge-storage-*.cy.js'",
    "test:streaming": "cypress run --spec 'cypress/e2e/api/data-streaming-*.cy.js'",
    "test:certificates": "cypress run --spec 'cypress/e2e/api/digital-certificates-*.cy.js'",
    "test:network-lists": "cypress run --spec 'cypress/e2e/api/network-lists-*.cy.js'",
    "test:workspace": "cypress run --spec 'cypress/e2e/api/workspace-*.cy.js'",
    "test:identity": "cypress run --spec 'cypress/e2e/api/identity-*.cy.js'",
    "test:payments": "cypress run --spec 'cypress/e2e/api/payments-*.cy.js'",
    "test:edge-functions": "cypress run --spec 'cypress/e2e/api/edge-functions-*.cy.js'",
    "test:low-priority": "cypress run --spec 'cypress/e2e/api/low-priority-*.cy.js'",
    "test:100-percent": "cypress run --spec 'cypress/e2e/api/**/*.cy.js'",
    "test:smoke": "cypress run --env grepTags='@smoke'",
    "test:regression": "cypress run --env grepTags='@regression'",
    "test:boundary": "cypress run --env grepTags='@boundary'",
    "test:comprehensive": "cypress run --spec 'cypress/e2e/api/{orchestrator,edge_firewall,iam}-comprehensive.cy.js'",
    "lint": "eslint cypress/**/*.js --fix",
    "lint:check": "eslint cypress/**/*.js",
    "format": "prettier --write cypress/**/*.js",
    "format:check": "prettier --check cypress/**/*.js",
    "quality:check": "npm run lint:check && npm run format:check",
    "coverage:report": "nyc report --reporter=html --reporter=text",
    "sonar:scan": "sonar-scanner",
    "report:generate": "node scripts/test-report-generator.js",
    "validate:project": "node scripts/validate-project-structure.js",
    "audit:coverage": "node scripts/coverage-audit.js",
    "generate:tests": "node scripts/comprehensive-test-generator.js",
    "migrate:tests": "node scripts/migrate-existing-tests.js",
    "report:merge": "mochawesome-merge reports/**/*.json > reports/merged-report.json",
    "report:html": "marge reports/merged-report.json --reportDir reports --inline",
    "dashboard:start": "node scripts/real-time-reporter.js dashboard",
    "dashboard:report": "node scripts/real-time-reporter.js report",
    "dashboard:monitor": "node scripts/real-time-reporter.js monitor",
    "coverage:live": "npm run dashboard:start",
    "cleanup:project": "node scripts/comprehensive-project-cleanup.js",
    "validate:syntax": "node -c cypress/**/*.js",
    "health:check": "npm run lint:check && npm run validate:syntax",
    "maintenance:full": "npm run cleanup:project && npm run health:check"
  },
  "keywords": [
    "cypress",
    "api-testing",
    "azion",
    "automation",
    "parallel-testing",
    "quality-assurance"
  ],
  "author": "",
  "license": "ISC",
  "description": "Comprehensive Cypress API testing suite for Azion V4 API with parallel execution, quality tools, and advanced reporting",
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@cypress/code-coverage": "^3.12.18",
    "@cypress/grep": "^4.0.1",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "ajv-errors": "^3.0.0",
    "axios": "^1.6.0",
    "chai": "^4.3.10",
    "cypress": "^13.6.0",
    "cypress-real-events": "^1.12.0",
    "eslint": "^8.57.0",
    "eslint-plugin-cypress": "^2.15.1",
    "glob": "^10.3.10",
    "husky": "^8.0.3",
    "js-yaml": "^4.1.0",
    "lint-staged": "^15.2.0",
    "lodash": "^4.17.21",
    "mocha": "^10.2.0",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.3.0",
    "mochawesome-report-generator": "^6.2.0",
    "nyc": "^15.1.0",
    "prettier": "^3.1.1"
  }
}